{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "ssr": "node ./node_modules/pm2/bin/pm2 start ecosystem.config.json",
        "prepare": "husky install",
        "type-check": "vue-tsc --noEmit",
        "test:watch": "vitest",
        "test": "vitest --run",
        "format": "prettier \"resources/**/*.{js,ts,vue}\" --write",
        "lint": "eslint \"resources/**/*.{js,ts,vue}\" --fix",
        "build": "npm run format && npm run lint && npm run type-check && vite build && vite build --ssr",
        "build:prod": "vue-tsc && vite build && vite build --ssr"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^18.6.0",
        "@fullcalendar/daygrid": "^6.1.10",
        "@fullcalendar/interaction": "^6.1.10",
        "@fullcalendar/timegrid": "^6.1.10",
        "@iconify/json": "^2.2.181",
        "@iconify/vue": "^4.1.1",
        "@inertiajs/vue3": "^1.0.14",
        "@tailwindcss/forms": "^0.5.7",
        "@types/ziggy-js": "^1.8.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vue/server-renderer": "^3.2.31",
        "autoprefixer": "^10.4.17",
        "axios": "^1.6.7",
        "commitlint": "^18.6.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-tailwindcss": "^3.14.2",
        "eslint-plugin-unused-imports": "^3.0.0",
        "eslint-plugin-vue": "^9.21.1",
        "laravel-vite-plugin": "^0.8.1",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "vite": "^4.5.2",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.34.6",
        "vue": "^3.2.41",
        "vue-tsc": "^1.8.27"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.10",
        "@fullcalendar/vue3": "^6.1.10",
        "@vueuse/components": "^10.7.2",
        "@vueuse/core": "^10.6.1",
        "dayjs": "^1.11.10",
        "husky": "^8.0.3",
        "pm2": "^5.3.1",
        "primevue": "^3.48.1"
    }
